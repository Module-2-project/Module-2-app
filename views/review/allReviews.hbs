<button onclick="history.back()"><img class="arrow-back" src="/images/back-arrow.png" /> Go back</button>
<h2>All reviews</h2>
<hr class="title-hr">

{{#if review}}
  {{#each review}}
    {{> adminReviewCard}}
  {{/each}}
{{else}}
  <p>There seems to be no reviews added.</p>
{{/if}}
{{#if error}}
  <p style="color:rgb(218, 90, 90)">{{ error }}</p>
{{/if}}
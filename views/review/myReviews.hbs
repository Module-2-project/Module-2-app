<button id="back-btn" onclick="history.back()"><img class="arrow-back" src="/images/back-arrow.png" /> Back</button>
<div class="gradient">
  <h2 class="titles">My reviews</h2>
</div>

{{#if review}}
  {{#each review}}
    {{> myReviewCard}}
  {{/each}}
{{else}}
  <p>It seems you didn't rate any recipe yet.</p>
{{/if}}
{{#if error}}
  <p style="color:rgb(218, 90, 90)">{{ error }}</p>
{{/if}}